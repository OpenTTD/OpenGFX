switch( FEAT_CANALS, SELF, switch_riverbank_height_temperate, tile_height) {
	0: set_rivermouth_temperate;
	set_riverbank_temperate;
}
switch( FEAT_CANALS, SELF, switch_riverbank_height_tropical, tile_height) {
	0: set_rivermouth_tropical;
	set_riverbank_tropical;
}
switch( FEAT_CANALS, SELF, switch_riverbank_height_arctic, tile_height) {
	0: set_rivermouth_arctic;
	set_riverbank_arctic;
}
switch( FEAT_CANALS, SELF, switch_riverbank_height_snow, tile_height) {
	0: set_rivermouth_snow;
	set_riverbank_snow;
}
switch( FEAT_CANALS, SELF, switch_riverbank_height_toyland, tile_height) {
	0: set_rivermouth_toyland;
	set_riverbank_toyland;
}

switch( FEAT_CANALS, SELF, switch_riverbank_terrain_arctic,
		[
			STORE_TEMP(                (tile_height > (snowline_height - 2)), 0),
			STORE_TEMP( LOAD_TEMP(0) + (tile_height > (snowline_height - 1)), 0),
			STORE_TEMP( LOAD_TEMP(0) + (tile_height > (snowline_height    )), 0),
			STORE_TEMP( LOAD_TEMP(0) + (tile_height > (snowline_height + 1)), 0),
			LOAD_TEMP(0)
		] ) {
	0: switch_riverbank_height_arctic;
	1: set_riverbank_snow14;
	2: set_riverbank_snow24;
	3: set_riverbank_snow34;
	switch_riverbank_height_snow;
}
switch( FEAT_CANALS, SELF, switch_riverbank_climate, climate) {
	CLIMATE_ARCTIC:   switch_riverbank_terrain_arctic;
	CLIMATE_TROPICAL: switch_riverbank_height_tropical;
	CLIMATE_TOYLAND:  switch_riverbank_height_toyland;
	switch_riverbank_height_temperate;
}

item( FEAT_CANALS, riverbanks, WF_RIVERBANKS) {
	graphics {
		default: switch_riverbank_climate;
	}
}


